{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "summary": {
      "type": "text",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "category": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Media",
        "Community",
        "Podcast"
      ]
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "links": {
      "type": "json"
    },
    "Seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    }
  }
}
